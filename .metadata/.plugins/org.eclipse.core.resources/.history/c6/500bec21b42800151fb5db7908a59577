import java.awt.image.BufferedImage;
import java.io.File;
import java.io.IOException;

import javax.imageio.ImageIO;


public class LoadImage {

	
BufferedImage yellow = null;	
	
	public LoadImage(){
		
		
		try {
	    yellow = ImageIO.read(new File("Images//yellow.png"));
		} catch (IOException e) {
		}	
		
	}
	
	
}
